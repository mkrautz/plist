$w = new Workflows();

$laststate = file_get_contents($w-&gt;data()."/laststate.txt");

if( strcmp($laststate, "start") == 0){
	$w-&gt;result( "stop", "stop", "Alfred Time Keeper", "Stop timing...", 'icon.png', 'yes');
	$w-&gt;result( "start", "start", "Alfred Time Keeper", "Start timing...", 'icon.png', 'yes');
} else {
	$w-&gt;result( "start", "start", "Alfred Time Keeper", "Start timing...", ':icon.png', 'yes');
	$w-&gt;result( "stop", "stop", "Alfred Time Keeper", "Stop timing...", 'icon.png', 'yes');
