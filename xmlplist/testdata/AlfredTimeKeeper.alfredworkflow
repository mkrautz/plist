				<string>#Original Author : Camilo Olarte|colarte@telesat.com.co|Sept.2003
# Modification Author: Richard Guay - raguay@customct.com | Feb 2013
#
# Program: AlfredTimeKeeper.py
#
# Description:    This program reads the Alfred Time Keeper files and displays the times worked
#                          on each project for that day.  I modified the original program tkCalendar to 
#                          fit my needs.
#
import sys, string, calendar
import Tkinter
import time
import string
from os.path import expanduser

pathToData = expanduser("~/Library/Application Support/Alfred 2/Workflow Data/com.customct.AlfredTimeKeeper/")
year = time.localtime()[0]
month = time.localtime()[1]
day =time.localtime()[2]
strdate = "{}-{:0&gt;2}-{:0&gt;2}".format(str(year),str(month),str(day))

tk = Tkinter 

fnta = ("Times", 12)
fnt = ("Times", 14)
fntc = ("Times", 18, 'bold')

numJobs = 0
try:
    jobf = open(pathToData + "projects.txt")
    for job in jobf:
       numJobs = numJobs + 1

except IOError:
    numjobs = 1

ATKwidth = numJobs*50
if(ATKwidth &lt; 300):
    ATKwidth = 300
ATKheight = 200
maxHour = 12
strtitle = "Time Sheet Calendar"
strdays = "Mo     Tu     We     Th     Fr     Sa     Su"
dictmonths = {'1':'Jan','2':'Feb','3':'Mar','4':'Apr','5':'May',
'6':'Jun','7':'Jul','8':'Aug','9':'Sep','10':'Oct','11':'Nov', '12':'Dec'}

def getJobTime(job, date):
    tm = 0
    start = 0
    times = 0
    try:
        filename = pathToData + job + "_" + date + ".txt"
        jtf = open(filename)
        for jt in jtf:
            jt = jt.strip()
            tsp = string.split(jt,":")
            if(tsp[1] == "start"):
                start = int(tsp[0])
            else:
                tm = tm + (int(tsp[0]) - start)
                start = 0
        if(start != 0):
            tm = tm + (time.time() - start)
        times = tm
    except IOError:
        times = 0
    return(int(times))


##############################################
#  BEGIN CLASS
class tkCalendar :
  def __init__ (self, master, arg_year, arg_month, arg_day, 
	    arg_parent_updatable_var, update_cmd):
    self.update_var = arg_parent_updatable_var
    self.update_cmd = update_cmd
    top = self.top = master
    try : self.intmonth = int(arg_month)
    except: self.intmonth = int(1)
    self.canvas =tk.Canvas (top, width =ATKwidth, height =ATKheight,
      relief =tk.RIDGE, background ="white", borderwidth =1)
    self.canvas.create_rectangle(0,0,303,30, fill="#a4cae8",width=0 )
    self.canvas.create_text(150,17, text=strtitle,  font=fntc, fill="#2024d6")
    stryear = str(arg_year)

    self.year_var=tk.StringVar()
    self.year_var.set(stryear)
    self.lblYear = tk.Label(top, textvariable = self.year_var, 
	    font = fnta, background="white")
    self.lblYear.place(x=135, y = 263)

    self.month_var=tk.StringVar()
    strnummonth = str(self.intmonth)
    strmonth = dictmonths[strnummonth]
    self.month_var.set(strmonth)

    self.lblYear = tk.Label(top, textvariable = self.month_var, 
	    font = fnta, background="white")
    self.lblYear.place(x=135, y = 283)
    #Variable muy usada
    tagBaseButton = "Arrow"
    self.tagBaseNumber = "DayButton"
    #draw year arrows
    x,y = 90, 43
    tagThisButton = "leftyear"  
    tagFinalThisButton = tuple((tagBaseButton,tagThisButton))
    self.fnCreateLeftArrow(self.canvas, x,y, tagFinalThisButton)
    x,y = 200, 43
    tagThisButton = "rightyear"  
    tagFinalThisButton = tuple((tagBaseButton,tagThisButton))
    self.fnCreateRightArrow(self.canvas, x,y, tagFinalThisButton)
    #draw month arrows
    x,y = 90, 63
    tagThisButton = "leftmonth"  
    tagFinalThisButton = tuple((tagBaseButton,tagThisButton))
    self.fnCreateLeftArrow(self.canvas, x,y, tagFinalThisButton)
    x,y = 200, 63
    tagThisButton = "rightmonth"  
    tagFinalThisButton = tuple((tagBaseButton,tagThisButton))
    self.fnCreateRightArrow(self.canvas, x,y, tagFinalThisButton)
    #Print days 
    self.canvas.create_text(150,90, text=strdays, font=fnta)
    self.canvas.pack (expand =1, fill =tk.BOTH)
    self.canvas.tag_bind ("Arrow", "&lt;ButtonRelease-1&gt;", self.fnClick)
    self.canvas.tag_bind ("Arrow", "&lt;Enter&gt;", self.fnOnMouseOver)
    self.canvas.tag_bind ("Arrow", "&lt;Leave&gt;", self.fnOnMouseOut)   
    self.fnFillCalendar()

  def fnCreateRightArrow(self, canv, x, y, strtagname):
    canv.create_polygon(x,y, [[x+0,y-5], [x+10, y-5] , [x+10,y-10] , 
		[x+20,y+0], [x+10,y+10] , [x+10,y+5] , [x+0,y+5]],
		tags = strtagname , fill="blue", width=0)

  def fnCreateLeftArrow(self, canv, x, y, strtagname):
    canv.create_polygon(x,y, [[x+10,y-10], [x+10, y-5] , [x+20,y-5] , 
		[x+20,y+5], [x+10,y+5] , [x+10,y+10] ],
		tags = strtagname , fill="blue", width=0)

  def fnClick(self,event):
    owntags =self.canvas.gettags(tk.CURRENT)
    if "rightyear" in owntags:
	intyear = int(self.year_var.get())
	intyear +=1
	stryear = str(intyear)
	self.year_var.set(stryear)
    if "leftyear" in owntags:
	intyear = int(self.year_var.get())
	intyear -=1
	stryear = str(intyear)
	self.year_var.set(stryear)
    if "rightmonth" in owntags:
	if self.intmonth &lt; 12 :
	    self.intmonth += 1 
	    strnummonth = str(self.intmonth)
	    strmonth = dictmonths[strnummonth]
	    self.month_var.set(strmonth)
	else :
	    self.intmonth = 1 
	    strnummonth = str(self.intmonth)
	    strmonth = dictmonths[strnummonth]
	    self.month_var.set(strmonth)
	    intyear = int(self.year_var.get())
	    intyear +=1
	    stryear = str(intyear)
	    self.year_var.set(stryear)
    if "leftmonth" in owntags:
	if self.intmonth &gt; 1 :
	    self.intmonth -= 1 
	    strnummonth = str(self.intmonth)
	    strmonth = dictmonths[strnummonth]
	    self.month_var.set(strmonth)
	else :
	    self.intmonth = 12
	    strnummonth = str(self.intmonth)
	    strmonth = dictmonths[strnummonth]
	    self.month_var.set(strmonth)
	    intyear = int(self.year_var.get())
	    intyear -=1
	    stryear = str(intyear)
	    self.year_var.set(stryear)
    self.fnFillCalendar()	    
    
  def fnFillCalendar(self):
    init_x_pos = 70
    arr_y_pos = [110,130,150,170,190,210]
    intposarr = 0
    self.canvas.delete("DayButton")
    self.canvas.update()
    intyear = int(self.year_var.get())
    monthcal = calendar.monthcalendar(intyear, self.intmonth)    
    for row in monthcal:
	xpos = init_x_pos 
	ypos = arr_y_pos[intposarr]
	for item in row:	
	    stritem = str(item)
	    if stritem == "0":
		xpos += 27
	    else :
		tagNumber = tuple((self.tagBaseNumber,stritem))
		self.canvas.create_text(xpos, ypos , text=stritem, 
		    font=fnta,tags=tagNumber)	
    		xpos += 27
	intposarr += 1
    self.canvas.tag_bind ("DayButton", "&lt;ButtonRelease-1&gt;", self.fnClickNumber)
    self.canvas.tag_bind ("DayButton", "&lt;Enter&gt;", self.fnOnMouseOver)
    self.canvas.tag_bind ("DayButton", "&lt;Leave&gt;", self.fnOnMouseOut)   

  def fnClickNumber(self,event):
    owntags =self.canvas.gettags(tk.CURRENT)
    for x in owntags:
        if (x == "current") or (x == "DayButton"): pass
        else : 
            strdate = "{}-{:0&gt;2}-{:0&gt;2}".format(str(self.year_var.get()),str(self.intmonth),str(x)) 
            self.update_var.set(strdate)
            self.update_cmd(strdate)

  def fnOnMouseOver(self,event):
    self.canvas.move(tk.CURRENT, 1, 1)
    self.canvas.update()

  def fnOnMouseOut(self,event):
    self.canvas.move(tk.CURRENT, -1, -1)
    self.canvas.update()

#  END CLASS
##############################################

class clsMainFrame(tk.Frame):
    def __init__(self, master):
	self.parent = master
	tk.Frame.__init__(master)
	self.date_var = tk.StringVar()
	self.date_var.set(strdate) 
        self.work = tk.Canvas(master, width =ATKwidth, height =ATKheight, relief =tk.RIDGE, background ="white", borderwidth =1)
        self.work.pack(side="top")
	label = tk.Label(master, textvariable= self.date_var, bg = "white")
	label.pack(side="top")
        self.cal = tkCalendar(master, year, month, day, self.date_var, self.update_cmd)
	exitBtn = tk.Button(master, text = 'Exit', command = master.quit)
	exitBtn.pack(side = 'right')

    def update_cmd(self, date):
        self.work.delete('all')
        jobs = []
        times = []
        numJobs = 0
        try:
            jobf = open(pathToData + "projects.txt")
            for job in jobf:
               job = job.strip()
               jobs.append(job)
               numJobs = numJobs + 1
               times.append(getJobTime(job,date))

            width = ATKwidth/numJobs
            bottomline = ATKheight - 8
            topOffset = 8
            botomHeight = ATKheight
            for colnum in range(1,numJobs+1):
               mn = times[colnum-1]/60
               hr = mn/60
               mn = mn - (60*hr)
               jtime = "{}:{:0&gt;2}".format(hr,mn)
               columnStart = int((1.0-((hr+(mn/60.0))/maxHour))*ATKheight)
               self.work.create_rectangle(((colnum-1)*width), columnStart, (width*colnum), botomHeight, fill="blue")
               self.work.create_text(((width/2) + ((colnum-1)*width)),bottomline,text=jobs[colnum-1], fill="white")
               self.work.create_text(((width/2) + ((colnum-1)*width)),(columnStart+topOffset),text=jtime, fill="white")
        except IOError:
            pass

root =tk.Tk()
root.title ("Time Sheet")
Frm = tk.Frame(root)
clsMainFrame(Frm)
Frm.pack()
root.mainloop()</string>
