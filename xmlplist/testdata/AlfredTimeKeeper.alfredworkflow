error_reporting(0);
$wf = new Workflows();
$proj = "{query}";
$oldproj=file_get_contents($wf-&gt;data()."/project.txt");
$oldproj=substr($oldproj,0,-1);
file_put_contents($wf-&gt;data()."/project.txt",$proj);
$last=file_get_contents($wf-&gt;data()."/laststate.txt");
$time=time();
$date=date('Y-m-d',$time);
if(strcmp($last,"start") == 0) {
   $fh = fopen($wf-&gt;data()."/{$oldproj}_{$date}.txt", 'a');
   fwrite($fh, "{$time}:stop\n");
   fclose($fh);
}

$proj = substr($proj,0,-1);
$fh = fopen($wf-&gt;data()."/{$proj}_{$date}.txt", 'a');
fwrite($fh, "{$time}:start\n");
fclose($fh);
file_put_contents($wf-&gt;data()."/laststate.txt","start");

echo "The current project is {$proj} and is started.";</string>
